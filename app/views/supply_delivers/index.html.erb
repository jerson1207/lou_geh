<p id="notice"><%= notice %></p>
<div class="header">
  Supply Delivery
  <%= link_to 'New Supply Deliver', new_supply_deliver_path, class: 'btn-create' %>
</div>
<table class = "table">
  <thead>
    <tr>
      <th>Unique Code</th>
      <th>Company</th>
      <th>Total Item(s)</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @supply_delivers.each do |supply_deliver| %>
      <tr>
        <td><%= supply_deliver.company_id %></td>
        <td><%= supply_deliver.company.name %></td>
        <td><%= supply_deliver.items.count %></td>
        <td><%= link_to 'View Item', supply_deliver %></td>
        <td><%= link_to 'Edit Name', edit_supply_deliver_path(supply_deliver) %></td>
        <td><%= link_to 'Destroy', supply_deliver, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

